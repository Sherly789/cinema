@{Layout = "Views/Shared/_Layout.cshtml";}
<h1>Cinema Scene</h1>
<hr />
<h2>Theaters:</h2>
@if (Model.Count != 0)
{
  <ul>
    @foreach (var theater in Model)
    {
      <li><a href='/theaters/@theater.GetId()'>@theater.GetLocation()</a></li>
    }
  </ul>
}

<h4><a href="/" class="btn btn-default">Home</a></h4>
